{
  "name": "chroMOVIE: New movie GIFs on every new tab!",
  "version": "0.1",
  "manifest_version": 2,
  "description": "You love movies? You love GIFs? You will love chroMOVIE!",
  "homepage_url": "https://github.com/liveashish/chroMOVIE",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "default_locale": "en",
  "background": {
    "persistent": true,
    "scripts": [
      "/js/jquery/jquery.min.js",
      "/src/bg/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "tabs"
  ]

}